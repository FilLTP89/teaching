@Article{Karapiperis_et_al_2016,
  Title                    = {{Fokker-Planck linearization for non-Gaussian stochastic elastoplastic finite elements}},
  Author                   = {Konstantinos Karapiperis and Kallol Sett and M. Levent Kavvas and Boris Jeremi\'{c}},
  Journal                  = {Computer Methods in Applied Mechanics and Engineering },
