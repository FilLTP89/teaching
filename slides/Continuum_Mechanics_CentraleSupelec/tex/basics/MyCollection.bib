@Article{Paolucci_1997,
  Title                    = {{Simplified Evaluation of Earthquake-Induced Permanent Displacements of Shallow Foundations}},
  Author                   = {Paolucci, Roberto},
  Journal                  = {Journal of Earthquake Engineering},
  Year                     = {1997},
  Number                   = {January, 1997},
  Pages                    = {37-41},

  Abstract                 = {The earthquake response of shallow foundations is studied by a simple method that incorporates nonlinear behaviour at the soil-foundation interface and, unlike other simplified approaches, directly takes into account the coupling between the soil-foundation system and the superstructure. The proposed model consists of a four degrees-of-freedom (dof) oscillator, namely one dof for the superstructure and three for the foundation, and introduces nonlinear effects in the calculation of soil reactions through a failure criterion and plastic flow-rule calibrated through previous experimental work. Comparing the results of an application to granular soils with those of more sophisticated finite element analyses, a satisfactory agreement is found, suggesting that the main features of this complicated nonlinear problem can be captured without resorting to an accurate description of the soil behaviour.$\backslash$nThe earthquake response of shallow foundations is studied by a simple method that incorporates nonlinear behaviour at the soil-foundation interface and, unlike other simplified approaches, directly takes into account the coupling between the soil-foundation system and the superstructure. The proposed model consists of a four degrees-of-freedom (dof) oscillator, namely one dof for the superstructure and three for the foundation, and introduces nonlinear effects in the calculation of soil reactions through a failure criterion and plastic flow-rule calibrated through previous experimental work. Comparing the results of an application to granular soils with those of more sophisticated finite element analyses, a satisfactory agreement is found, suggesting that the main features of this complicated nonlinear problem can be captured without resorting to an accurate description of the soil behaviour.},
  Doi                      = {10.1080/13632469708962378},
  File                     = {Paolucci_1997.pdf:Paolucci_1997.pdf:PDF},
  Optiisbn                 = {1363246970896},
  Optissn                  = {1363-2469}
}

@Article{Paolucci_et_al_2013,
  Title                    = {{Introducing Dynamic Nonlinear Soil-Foundation-Structure Interaction Effects in Displacement-Based Seismic Design}},
  Author                   = {Paolucci, Roberto and Figini, Raffaele and Petrini, Lorenza},
  Journal                  = {Earthquake Spectra},
  Year                     = {2013},
  Number                   = {2},
  Pages                    = {475-496},
  Volume                   = {29},

  Abstract                 = {An iterative linear-equivalent procedure to take into account nonlinear soil-structure interaction effects in the displacement-based seismic design is presented for the case of shallow foundations. The procedure is based on the use of empirical curves to evaluate the stiffness degradation and the increase of damping ratio as a function of foundation rotation. Iterations are performed to ensure that admissible values of foundation rotations are complied with, in addition to the standard checks on structural displacements and drifts. Some examples of application of the approach to the design of bridge piers are provided. Design results are checked by means of nonlinear dynamic time-history analyses performed by a macro-element-based numerical tool, assuming nonlinear behavior of both structure and soil-foundation system.},
  Doi                      = {10.1193/1.4000135},
  File                     = {Paolucci_et_al_2013.pdf:Paolucci_et_al_2013.pdf:PDF},
  Optissn                  = {8755-2930}
