@Article{Proctor_et_al_2014,
  Title                    = {{Dynamic weakening of serpentinite gouges and bare surfaces at seismic slip rates}},
  Author                   = {Proctor, B. P. and Mitchell, T. M. and Hirth, G. and Goldsby, D. and Zorzi, F. and Platt, J. D. and Toro, G. Di},
  Journal                  = {Journal of Geophysical Research: Solid Earth},
  Year                     = {2014},

  Month                    = {11},
  Number                   = {11},
  Pages                    = {8107--8131},
  Volume                   = {119},

  Abstract                 = {To investigate differences in the frictional behavior between initially bare rock surfaces of serpentinite and powdered serpentinite (?gouge?) at subseismic to seismic slip rates, we conducted single-velocity step and multiple-velocity step friction experiments on an antigorite-rich and lizardite-rich serpentinite at slip rates (V) from 0.003?m/s to 6.5?m/s, sliding displacements up to 1.6?m, and normal stresses (?n) up to 22?MPa for gouge and 97?MPa for bare surfaces. Nominal steady state friction values (?nss) in gouge at V?=?1?m/s are larger than in bare surfaces for all ?n tested and demonstrate a strong ?n dependence; ?nss decreased from 0.51 at 4.0?MPa to 0.39 at 22.4?MPa. Conversely, ?nss values for bare surfaces remained ~0.1 with increasing ?n and V. Additionally, the velocity at the onset of frictional weakening and the amount of slip prior to weakening were orders of magnitude larger in gouge than in bare surfaces. Extrapolation of the normal stress dependence for ?nss suggests that the behavior of antigorite gouge approaches that of bare surfaces at ?n???60?MPa. X-ray diffraction revealed dehydration reaction products in samples that frictionally weakened. Microstructural analysis revealed highly localized slip zones with melt-like textures in some cases gouge experiments and in all bare surfaces experiments for V???1?m/s. One-dimensional thermal modeling indicates that flash heating causes frictional weakening in both bare surfaces and gouge. Friction values for gouge decrease at higher velocities and after longer displacements than bare surfaces because strain is more distributed.},
  Doi                      = {10.1002/2014JB011057},
  File                     = {Proctor_et_al_2014.pdf:Proctor_et_al_2014.pdf:PDF},
  ISSN                     = {2169-9356},
  Publisher                = {Wiley Online Library},
  Url                      = {http:https://doi.org/10.1002/2014JB011057}
}

