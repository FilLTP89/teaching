@Article{Bard_Bouchon_1980,
  Title                    = {{The seismic response of sediment-filled valleys. Part 1. The case of incident SH waves}},
  Author                   = {Bard, P.-Y. and Bouchon, M.},
  Journal                  = {Bulletin of the Seismological Society of America},
  Year                     = {1980},
