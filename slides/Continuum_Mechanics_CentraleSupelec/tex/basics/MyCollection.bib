@Article{Derbal2017,
  author    = {Ismail Derbal and Nouredine Bourahla and Ahmed Mebarki and Ramdane Bahar},
  title     = {{Neural network-based prediction of ground time history responses}},
  journal   = {European Journal of Environmental and Civil Engineering},
  year      = {2017},
  volume    = {0},
  number    = {0},
  pages     = {1-18},
  abstract  = { AbstractThis article proposes a method based on artificial neural networks (ANN) to predict the time history responses at any point on the ground near a shaking table reaction mass (Earthquake Engineering Research Centre, Algiers). The induced vibrations may disturb occupants or damage surrounding buildings. The accelerations generated by the shaking table are used as inputs. The reaction mass and the surrounding soil are modelled using COMSOL Multiphysics software. The natural frequencies were validated experimentally in order to determine the optimum model soil extent. Twenty-one measurements at equal distances from the source were retrieved from the results of the numerical analysis to feed a database with outputs (targets) for a back-propagation feed-forward neural. First, an initial architecture with 2 inputs reached .87 as correlation factor for signal prediction and a maximal error of 17\% for peaks prediction. In order to improve the ANN model, a synchronisation of the input?output signals has been proposed using the cross-correlation product to determine the offset. Further enhancement of the ANN has been achieved by introducing 5 time delays before and after each signal time step. The resulting ANN with 12 inputs reached almost 100\% correlation and less than 2\% error for the peaks prediction. },
  doi       = {10.1080/19648189.2017.1367727},
  eprint    = {https://doi.org/10.1080/19648189.2017.1367727},
  publisher = {Taylor \& Francis},
  url       = { 
        https://doi.org/10.1080/19648189.2017.1367727
    
},
}
