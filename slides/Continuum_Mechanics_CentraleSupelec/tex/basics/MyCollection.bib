@Article{MontoyaNoguera_Wang_2017,
  Title                    = {{Bayesian identification of multiple seismic change points and varying seismic rates caused by induced seismicity}},
  Author                   = {Montoya-Noguera, Silvana and Wang, Yu},
  Journal                  = {Geophysical Research Letters},
  Year                     = {2017},
  Note                     = {2016GL072266},
  Number                   = {8},
  Pages                    = {3509--3516},
  Volume                   = {44},

  Abstract                 = {The Central and Eastern United States (CEUS) has experienced an abnormal increase in seismic activity, which is believed to be related to anthropogenic activities. The U.S. Geological Survey has acknowledged this situation and developed the CEUS 2016 1 year seismic hazard model using the catalog of 2015 by assuming stationary seismicity in that period. However, due to the nonstationary nature of induced seismicity, it is essential to identify change points for accurate probabilistic seismic hazard analysis (PSHA). We present a Bayesian procedure to identify the most probable change points in seismicity and define their respective seismic rates. It uses prior distributions in agreement with conventional PSHA and updates them with recent data to identify seismicity changes. It can determine the change points in a regional scale and may incorporate different types of information in an objective manner. It is first successfully tested with simulated data, and then it is used to evaluate Oklahoma's regional seismicity.},
  Doi                      = {10.1002/2016GL072266},
  File                     = {MontoyaNoguera_Wang_2017.pdf:MontoyaNoguera_Wang_2017.pdf:PDF},
  ISSN                     = {1944-8007},
  Keywords                 = {Probabilistic forecasting, Uncertainty quantification, Statistical analysis, Earthquake ground motions and engineering seismology, Earthquake interaction, forecasting, and prediction, Bayesian methods, induced seismicity, multiple change point method, seismic hazard},
  Owner                    = {filippo},
